{
  "parser": "@babel/eslint-parser",
  "extends": [
    "eslint:recommended",
    "alloy",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": 1,
    "prefer-destructuring": 1,
    "default-case": 2,
    "eqeqeq": 2,
    "no-alert": 2,
    "no-unused-vars": 0,
    "no-unreachable": 2,
    "no-duplicate-imports": 2,
    "max-params":["error", 8],
    "complexity": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
